<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Party Cafe закрытая зона - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
</head>
<body>

    <!-- BG-VIDEO -->
    <div class="bgvideo">
        <video id="vplayer">
            <source src="~/Content/video/video.mp4" type="video/mp4" />
            <source src="~/Content/video/video.webm" type="video/webm" />
            <source src="~/Content/video/video.ogv" type="video/ogg" />
        </video>
        <script>
            function p() {
                var video = document.getElementById('vplayer');
                video.loop = true;
                video.autoplay = true;
                video.play();
            }
            p();
        </script>
    </div>
    <!--/ BG-VIDEO -->

    <!-- CONTENT -->
    <div class="content">
        <header class="header">
            <nav id="navigation">
                @Html.Partial("_Menu")
            </nav>
        </header>

        <div class="wrap">
            <div>
                @RenderBody()
            </div>
        </div>

        <footer class="footer">
            <div class="f-wrap">
                <p>@DateTime.Now.Year &copy; All Rights reserved by <a href="/">Party-Cafe</a></p>
            </div>
        </footer>
    </div>
    <!--/ CONTENT -->



